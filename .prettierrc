{
   "$schema": "https://json.schemastore.org/prettierrc",
   "tabWidth": 3,
   "semi": false,
   "singleQuote": true,
   "trailingComma": "none",
   "printWidth": 120,
   "useTabs": false,
   "bracketSpacing": true,
   "bracketSameLine": false,
   "arrowParens": "always",
   "singleAttributePerLine": true,
   "jsxSingleQuote": false,
   "quoteProps": "consistent"
}
